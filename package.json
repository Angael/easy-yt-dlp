{
  "name": "easy-yt-dlp",
  "version": "0.4.0",
  "description": "NPM library for easy usage of yt-dlp",
  "keywords": [
    "yt-dlp",
    "npm"
  ],
  "homepage": "https://github.com/Angael/easy-yt-dlp",
  "author": {
    "name": "Krzysztof Widacki",
    "email": "krzysztofwidacki@gmail.com",
    "url": "https://krzysztof.widacki.me/"
  },
  "repository": {
    "url": "https://github.com/Angael/easy-yt-dlp.git",
    "type": "git"
  },
  "private": false,
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "test": "cross-env YTDLP_PATH=\"yt-dlp\" jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build && yarn test",
    "preversion": "yarn build && yarn generate-docs",
    "postversion": "git push --follow-tags",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown --readme none --out docs src/index.ts",
    "generate-docs:watch": "typedoc --watch --plugin typedoc-plugin-markdown --readme none --out docs src/index.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typedoc": "0.24.7",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=v18.12.1"
  },
  "dependencies": {}
}
